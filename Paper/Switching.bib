journal = {PLoS ONE},
number = {12},
pmid = {27907014},
title = {{Classification and verification of handwritten signatures with time causal information theory quantifiers}},
% url = {http://journals.plos.org/plosone/article/file?id=10.1371/journal.pone.0166868{\&}type=printable},
volume = {11},
