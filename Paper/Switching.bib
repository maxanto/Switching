@book{Frieden2004,
abstract = {This book develops and applies an analytical approach to deriving the probability laws of science in general. It is called 'extreme physical information' or EPI. EPI is an expression of the imperfection of observation: Owing to random interaction of a subject with its observer and other possible disturbances, its measurement contains less Fisher information than does the subject per se. Moreover, the information loss is an extreme value. An EPI output may alternatively be viewed as the payoff of a zero-sum game of information acquisition between the observer and a 'demon' in subject space. EPI derives, Escher-like, the very probability law that gave rise to the measurement. In applications, EPI is used to derive both existing and new analytical relations governing probability laws of physics, genetics, cancer growth, ecology and economics. This unified approach will be fascinating to students and those who seek a new mathematical tool of research.},
author = {Frieden, B. Roy},
isbn = {0521009111},
keywords = {0521009111$\backslash$r$\backslash$n9780521009119},
pages = {490},
publisher = {Cambridge University Press},
title = {{Science from Fisher Information: A Unification}},
% url = {https://books.google.com.ar/books?hl=es{\&}lr={\&}id=qoFW76lXlEMC{\&}oi=fnd{\&}pg=PA1{\&}dq=Frieden+BR.+Science+from+Fisher+information:+a+unification.+Cambridge+University+Press{\%}3B+2004.+26.{\&}ots=nFcuuRDXgP{\&}sig=A1kFbuO28VC-yjtMlKnyQTkO-Zo{\#}v=onepage{\&}q=Frieden BR. Science},
year = {2004}
}
@article{Fadlallah2013,
abstract = {Permutation entropy (PE) has been recently suggested as a novel measure$\backslash$nto characterize the complexity of nonlinear time series. In this$\backslash$npaper, we propose a simple method to address some of PE's limitations,$\backslash$nmainly its inability to differentiate between distinct patterns of$\backslash$na certain motif and the sensitivity of patterns close to the noise$\backslash$nfloor. The method relies on the fact that patterns may be too disparate$\backslash$nin amplitudes and variances and proceeds by assigning weights for$\backslash$neach extracted vector when computing the relative frequencies associated$\backslash$nwith every motif. Simulations were conducted over synthetic and real$\backslash$ndata for a weighting scheme inspired by the variance of each pattern.$\backslash$nResults show better robustness and stability in the presence of higher$\backslash$nlevels of noise, in addition to a distinctive ability to extract$\backslash$ncomplexity information from data with spiky features or having abrupt$\backslash$nchanges in magnitude},
author = {Fadlallah, Bilal and Chen, Badong and Keil, Andreas and Pr{\'{i}}ncipe, Jos{\'{e}}},
doi = {10.1103/PhysRevE.87.022911},
file = {:C$\backslash$:/Users/Maxi/Downloads/fadlallah2013.pdf:pdf},
issn = {15393755},
journal = {Physical Review E - Statistical, Nonlinear, and Soft Matter Physics},
