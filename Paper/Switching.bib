doi = {10.1016/j.cnsns.2016.05.002},
file = {:C$\backslash$:/Users/Maxi/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Antonelli, De Micco, Larrondo - 2016 - Measuring the jitter of ring oscillators by means of information theory quantifiers.pdf:pdf},
issn = {10075704},
journal = {Commun. Nonlinear Sci. Numer. Simul.},
keywords = {Entropy,FPGA,Jitter,Ring oscillators},
pages = {139--150},
title = {{Measuring the jitter of ring oscillators by means of information theory quantifiers}},
volume = {43},
year = {2017}
}
@article{Azzaz2013,
abstract = {In this paper, a new stream key generator Hybrid Discrete Continuous Chaotic System (HDCCS) based on continuous and discrete chaotic systems is proposed. Our solution provides an easy and robust chaos synchronization while decrease the degradation due to finite precision during a digital implementation. Real-time application to wireless speech encryption effectively hides the original signal. Statistical security analysis for the proposed cryptosystem against cryptanalysis attacks, from a strict cryptographic viewpoint, are presented. These results verify and prove that the proposed speech cryptosystem is highly secure and has a very powerful diffusion and confusion mechanisms widely used in conventional cryptography. {\textcopyright} 2012 Elsevier B.V.},
annote = {However, the hardware realization of these sequence generators with good statistical properties is still a great challenge. Furthermore, when chaotic generators are implemented in a digital form with finite precision, it is doubtful whether or not they can still preserve the desired dynamics of the chaotic systems since most dynamical properties of chaos are mean- ingful only when dynamical systems evolve in the continuous phase space. These properties may become ambiguous when the phase space is forcedly digitized with a finite computing precision[17–19].

Hence, to design an efficient digital chaotic cipher, the following most essential drawbacks of the chaotic systems must be considered: (i) weak resistance to recovery of parameters, (ii) lower digital precision comparing to the analog generators, (iii) periodicity problems, (iv) non-ideal probability distributions and (v) higher level of correlation with a bad trade-off cost, per- formance and power consumption.

The continuous chaotic system disturbs the discrete chaotic system in order to eliminate the peri- odicity owing to the finite precision. In addition, this system is also used to achieve synchronization between the master HDCCS and the slave one.},
author = {Azzaz, Mohamed Salah and Tanougast, Camel and Sadoudi, Said and Bouridane, Ahmed},
doi = {10.1016/j.cnsns.2012.12.018},
file = {:C$\backslash$:/Users/Maxi/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Salah Azzaz et al. - 2013 - Synchronized hybrid chaotic generators Application to real-time wireless speech encryption.pdf:pdf},
issn = {10075704},
journal = {Commun. Nonlinear Sci. Numer. Simul.},
keywords = {Chaos,Cryptosystem,FPGA,NIST,Speech},
number = {8},
pages = {2035--2047},
title = {{Synchronized hybrid chaotic generators: Application to real-time wireless speech encryption}},
volume = {18},
year = {2013}
}
