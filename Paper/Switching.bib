@article{Fadlallah2013,
abstract = {Permutation entropy (PE) has been recently suggested as a novel measure$\backslash$nto characterize the complexity of nonlinear time series. In this$\backslash$npaper, we propose a simple method to address some of PE's limitations,$\backslash$nmainly its inability to differentiate between distinct patterns of$\backslash$na certain motif and the sensitivity of patterns close to the noise$\backslash$nfloor. The method relies on the fact that patterns may be too disparate$\backslash$nin amplitudes and variances and proceeds by assigning weights for$\backslash$neach extracted vector when computing the relative frequencies associated$\backslash$nwith every motif. Simulations were conducted over synthetic and real$\backslash$ndata for a weighting scheme inspired by the variance of each pattern.$\backslash$nResults show better robustness and stability in the presence of higher$\backslash$nlevels of noise, in addition to a distinctive ability to extract$\backslash$ncomplexity information from data with spiky features or having abrupt$\backslash$nchanges in magnitude},
